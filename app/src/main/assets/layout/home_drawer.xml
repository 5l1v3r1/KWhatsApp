<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <kmods.view.NavigationDrawerGoogle
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffff5252">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="210dp"
                android:id="@id/drawer_header_container">
                <ViewStub
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@id/drawer_header"
                    android:inflatedId="@id/drawer_header"/>
            </RelativeLayout>
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/drawer_header_container">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:paddingTop="5dp"
                    android:id="@id/drawer_buttons">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
						android:background="?selectableItemBackground"
                        android:clickable="true"
                        android:id="@id/drawer_newgroup">
                            <ImageView
                                android:layout_width="24dp"
                                android:layout_height="24dp"
                                android:src="@drawable/wamod_drawer_ic_newgroup"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="16dp"
                                android:padding="2dp"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#ffffff"
                                android:text="@string/new_group"
                                android:textSize="15sp"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="72dp"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
						android:background="?selectableItemBackground"
                        android:clickable="true"
                        android:id="@id/drawer_web">
                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/wamod_drawer_ic_wamodweb"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:padding="2dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#ffffff"
                            android:text="@string/whatsapp_web"
                            android:textSize="15sp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="72dp"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
						android:background="?selectableItemBackground"
                        android:clickable="true"
                        android:id="@id/drawer_setstatus">
                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/wamod_drawer_ic_status"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:padding="2dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#ffffff"
                            android:text="@string/settings_privacy_status"
                            android:textSize="15sp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="72dp"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
						android:background="?selectableItemBackground"
                        android:clickable="true"
                        android:id="@id/drawer_starredmessages">
                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/wamod_drawer_ic_starredmessages"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:padding="2dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#ffffff"
                            android:text="@string/starred_messages"
                            android:textSize="15sp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="72dp"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
						android:background="?selectableItemBackground"
                        android:clickable="true"
                        android:id="@id/drawer_archivedchats">
                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/wamod_drawer_ic_archivedchats"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:padding="2dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#ffffff"
                            android:text="@string/archived_chats"
                            android:textSize="15sp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="72dp"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
						android:background="?selectableItemBackground"
                        android:clickable="true"
                        android:id="@id/drawer_settings">
						<ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/drawer_settings"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:padding="2dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#ffffff"
                            android:text="@string/settings_general"
                            android:textSize="15sp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="72dp"/>
                    </RelativeLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#30ffffff"
                        android:layout_marginLeft="0dp"
                        android:layout_marginRight="0dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginBottom="4dp"
                        android:clickable="true" />
					<RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
						android:background="?selectableItemBackground"
                        android:clickable="true"
                        android:id="@id/drawer_privacy">
                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/wamod_drawer_ic_privacy"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:padding="2dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#ffffff"
                            android:text="@string/settings_privacy"
                            android:textSize="15sp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="72dp"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
						android:background="?selectableItemBackground"
                        android:clickable="true"
                        android:id="@id/drawer_modsettings">
						<ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/wamod_drawer_ic_wamodsettings"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="16dp"
                            android:padding="2dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#ffffff"
                            android:text="GeekMods"
                            android:textSize="15sp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="72dp"/>
                    </RelativeLayout>
                </LinearLayout>
            </ScrollView>
        </RelativeLayout>
    </kmods.view.NavigationDrawerGoogle>
</merge>